      crossorigin="anonymous"
    />
   

    <link rel="stylesheet" href="/css/style.css" />
    <link href="https://vjs.zencdn.net/7.14.3/video-js.css" rel="stylesheet" />
    <script src="https://vjs.zencdn.net/7.14.3/video.js"></script>
  </head>
  <body class="test">
    <header class="container fixed-header">
      <div class="header ">
        <%if(pageTitle == "gymstats"){ %>
        <a href="/feed"><h1 class="logo m-0"><%= pageTitle %></h1></a>
        <%}else if(pageTitle == loggedUser.userName.toString()){%>
            <a href="/editProfile/<%= loggedUser._id %>"><h1 class="logo m-0"> <%= pageTitle %><i class="fas fa-edit"></i></h1></a>
              <% }else{ %>
      
        <a href="javascript:history.back()"><h1 class="logo m-0"><%= pageTitle %></h1></a>
        <%}%>
        <div class="headerlinks d-flex justify-content-center align-items-center">
          <!-- header.ejs -->
         
          

          <div class="post-button">
            <a href="/users/notifications">
              <div class="notification-icon">
                <i class="fas fa-bell"></i>
                <%if( notificationsCount > 0 ){%>
                  <% if (!onNotificationsPage) { %>
                  <span class="notification-count"><%= notificationsCount %></span>
                  <% } %>
                <%}%>
              </div>
              <a href="/postmenu" class="">
                <div class="plus-icon">
                  <i class="fas fa-plus"></i>
                 </div>
              </a>
          </div>
        </div>
      </div>
    </header>
  </body>
  <script>
    </script>
</html>
